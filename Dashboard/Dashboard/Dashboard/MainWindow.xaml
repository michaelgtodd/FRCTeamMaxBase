<Window x:Class="Dashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dashboard"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <Grid Background="Gray">
        <Label Content="Incoming Osc Data" Margin="10, 0, 10, 0" FontSize="16" FontWeight="Bold"/>
        <DataGrid Name="OscDataGrid" ItemsSource="{Binding}" IsReadOnly="True" Margin="10, 30, 10, 10" Background="LightGray" HeadersVisibility="Column" ColumnWidth="*"/>
    </Grid>
</Window>
